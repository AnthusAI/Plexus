{
    "version": "0.2.0",
    "python": {
        "pythonPath": "/Users/ryan/miniconda3/envs/py39/bin/python"
    },
    "configurations": [

        {
            "name": "Plexus Evaluate Accuracy",
            "type": "python",
            "request": "launch",
            "module": "plexus",
            "args": [
                "evaluate",
                "accuracy",
                "--scorecard-name",
                "cs3_tpa_psd_in",
                "--score-name",
                "Good Call",
                "--number-of-samples",
                "10",
                "--sampling-method",
                "random",
                "--fresh",
                // "--visualize",
                // "--use-langsmith-trace",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/../Call-Criteria-Python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Plexus Predict",
            "type": "python",
            "request": "launch",
            "module": "plexus",
            "args": [
                "predict",
                "--scorecard-name", "termlifev1",
                "--score-name", "Carrier Access",
                "--number",
                "1"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/../Call-Criteria-Python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Plexus Score (DBI)",
