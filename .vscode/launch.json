{
    "version": "0.2.0",
    "python": {
        "pythonPath": "/Users/ryan/miniconda3/envs/py39/bin/python"
    },
    "configurations": [
        {
            "name": "Plexus Evaluate Accuracy",
            "type": "python",
            "request": "launch",
            "module": "plexus",
            "args": [
                "evaluate",
                "accuracy",
                "--scorecard-name",
                "cs3_tpa_psd_in",
                "--score-name",
                "Good Call",
                "--number-of-samples",
                "40",
                "--sampling-method",
                "random"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/../Call-Criteria-Python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Plexus Predict",
            "type": "python",
            "request": "launch",
            "module": "plexus",
            "args": [
                "predict",
                "--scorecard-name",
                "randallreillyv1",
                "--score-name",
                "IVR Present",
                "--number",
                "1"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/../Call-Criteria-Python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Plexus Generate Batch",
            "type": "python",
            "request": "launch",
            "module": "plexus",
            "args": [
                "batch",
                "generate",
